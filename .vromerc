map j 3j
map J <C-d>
map k 3k
map K <C-u>
map h gT
map l gt
map x dc

unmap w
unmap Y

set hintkeys=asdfghjkl;
set hintkeysdisplayuppercase=1

set nextpattern+=^后页|>>$
set previouspattern+=^前页|<<$

set disablesites=mail.google.com, www.google.com/reader, doit.im
set searchengines={"google":"https://www.google.com/search?hl=en&q={{keyword}}","yahoo":"http://search.yahoo.com/search?p={{keyword}}","w":"http://en.wikipedia.org/wiki/{{keyword}}","answers":"http://www.answers.com/main/ntquery?s={{keyword}}","yubnub":"http://yubnub.org/parser/parse?command={{keyword}}","b":"http://www.baidu.com/s?wd={{keyword}}","bing":"http://www.bing.com/search?setmkt=en-US&q={{keyword}}","tb":"http://s.taobao.com/search?q={{keyword}}","bk":"http://book.douban.com/subject_search?search_text={{keyword}}&cat=1001"}
set defaultsearch=google
set editor=gvim -f

set autocomplete_prev=<S-Tab>
set autocomplete_next=<Tab>
set autocomplete_prev_10=<Up>
set autocomplete_next_10=<Down>
